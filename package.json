    "scripts": {
        "start": "NODE_ENV=production bun load-env && bun build/index.js",
        "build": "bun load-env bun build --production --minify --target=bun --outdir=build server/index.ts",
        "build:docker": "docker build -t localoco .",
        "ingest": "gitingest ./",
        "load-env": "dotenv -e .env --",
        "dev:server": "bun run --hot server/index.ts",
        "dev:vite": "wait-on http://localhost:3000/health && bun load-env vite",
        "dev": "concurrently \"bun dev:server\" \"bun dev:vite\"",
        "auth:generate": "bunx load-env @better-auth/cli generate --output server/database/auth-schema.ts --config src/lib/auth.ts",
        "db:pull": "bun load-env drizzle-kit pull",
        "db:push": "bun load-env drizzle-kit push",
        "db:generate": "bun load-env drizzle-kit generate",
        "db:view": "bun load-env drizzle-kit studio",
        "lint:check": "eslint .",
        "lint:fix": "eslint . --fix",
        "format:check": "prettier --check .",
        "format:fix": "prettier --write .",
        "type:check": "tsc --noEmit",
        "validate": "concurrently --kill-others-on-fail \"bun run lint:check\" \"bun run type:check\" \"bun run format:check\"",
        "prepare": "husky",
        "clean": "git clean -xdf .cache .turbo build dist node_modules .tanstack"
    }
}
